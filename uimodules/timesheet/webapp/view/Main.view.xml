<mvc:View controllerName="trix.timesheet.controller.Main" displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core" core:require="{
		formatter: 'trix/timesheet/model/formatter'
	}"
	xmlns:controls="trix.timesheet.controls">

	<Page title="{i18n>appTitle}" id="page">
		<content>
			<controls:TRIXCalendar appointments="{PeriodRegistrations>/}" title= "TRIXY TRIX">
				<appointments>
					<unified:CalendarAppointment title= "{title}" text= "{text}" startDate= "{startDate}" endDate= "{endDate}">
					</unified:CalendarAppointment>
				</appointments>
			</controls:TRIXCalendar>
			<List items="{PeriodRegistrations>/}">
				<CustomListItem>
					<Text text="{ID}" />
				</CustomListItem>
			</List>
		</content>
	</Page>

</mvc:View>
