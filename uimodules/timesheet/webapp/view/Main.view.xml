<mvc:View
	controllerName="trix.timesheet.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	core:require="{
		formatter: 'trix/timesheet/model/formatter'
	}"
	xmlns:controls="trix.timesheet.controls">

	<Page
		title="{i18n>appTitle}"
		id="page">
		<content>
			<controls:TRIXCalendar title= "TRIXY TRIX" />
			<List items="{PeriodRegistrations>/}" >
				<CustomListItem>
					<Text text="{ID}" />
				</CustomListItem>
			</List>
		</content>
	</Page>

</mvc:View>
