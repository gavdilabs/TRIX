<mvc:View
	controllerName="trix.timesheet.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	core:require="{
		formatter: 'trix/timesheet/model/formatter'
	}"
	xmlns:controls="trix.timesheet.controls">

	<Page
		title="{i18n>appTitle}"
		id="page">
		<content>
			<controls:TRIXCalendar title= "TRIXY TRIX" enableAppointmentsCreate="true" enableAppointmentsDragAndDrop="true" firstDayOfWeek="1" scaleFactor="2" stickyMode="All" appointmentSelect="onAppointmentSelect" appointmentCreate="onNewTimeReg"/>
		</content>
	</Page>

</mvc:View>
