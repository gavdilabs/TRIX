{
	"name": "trix.timesheet",
	"version": "1.0.0",
	"description": "trix.timesheet",
	"author": "Gavdi Labs",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"start": "ui5 serve --port 8080 -o index.html",
		"start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov",
		"wdi5": "wdio run ./webapp/test/e2e/wdio.conf.ts",
		"ui5-interface-gen": "npx @ui5/ts-interface-generator --watch"
	},
	"devDependencies": {
		"@openui5/types": "^1.121.0",
		"@sapui5/types": "1.121.0",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@ui5/cli": "^3.10.1",
		"@wdio/cli": "^8.32.3",
		"@wdio/local-runner": "^8.32.3",
		"@wdio/mocha-framework": "^8.32.3",
		"@wdio/spec-reporter": "^8.32.2",
		"eslint": "^8.56.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^3.0.4",
		"karma-ui5-transpile": "^3.3.3",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"ui5-middleware-livereload": "^3.0.2",
		"ui5-middleware-simpleproxy": "^0.9.1",
		"ui5-tooling-transpile": "^3.3.3",
		"wdio-ui5-service": "^2.0.6",
		"@ui5/ts-interface-generator": "^0.8.2"
	},
	"ui5": {
		"dependencies": [
			"ui5-middleware-livereload",
			"ui5-middleware-simpleproxy",
			"ui5-tooling-modules"
		]
	}
}
